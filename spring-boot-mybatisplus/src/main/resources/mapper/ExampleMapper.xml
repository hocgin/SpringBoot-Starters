<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="in.hocg.mybatis.module.mapper.ExampleMapper">

    <sql id="columns">
        ${alias}.id,
        ${alias}.createdAt
    </sql>

    <select id="page1" resultType="in.hocg.mybatis.pojo.vo.ExampleVo">
        SELECT te.*
        FROM t_example te
    </select>

    <select id="page2" resultType="in.hocg.mybatis.pojo.vo.ExampleVo">
        SELECT te.created_at
        FROM t_example te
    </select>

    <select id="findFirst" resultType="in.hocg.mybatis.module.domain.Example">
        SELECT te.*
        FROM t_example te
        LIMIT 1
    </select>

    <select id="pageQuery" resultType="in.hocg.mybatis.pojo.vo.ExampleVo">
        SELECT te.created_at
        FROM t_example te
    </select>
</mapper>